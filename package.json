{
  "name": "conuai-tools",
  "version": "1.0.0",
  "private": true,
  "description": "SEO & AI Indexing Analysis Tools",
  "workspaces": ["frontend", "packages/*"],
  "scripts": {
    "dev": "bun run --cwd frontend dev",
    "build": "bun run --cwd frontend build",
    "lint": "bun run --cwd frontend lint",
    "check": "bun run --cwd frontend check",
    "typecheck": "bun run --cwd frontend typecheck",
    "analyze": "cd backend && python3 seo_analyzer.py",
    "generate": "cd backend && python3 seo_ai_generator.py"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8"
  }
}
